    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist" 
  },
  "include": ["./funcs/*.ts", "consts.ts"],
  "exclude": ["./node_modules", "package-lock.json"]
}